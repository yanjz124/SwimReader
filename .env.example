# FAA SCDS Connection Credentials
# Copy this file to .env and fill in your values
# Or use: dotnet user-secrets set "ScdsConnection:Username" "your-username"

SCDSCONNECTION__HOST=tcps://ems2.swim.faa.gov:55443
SCDSCONNECTION__USERNAME=your-scds-username
SCDSCONNECTION__PASSWORD=your-scds-password
SCDSCONNECTION__MESSAGEVPN=STDDS
SCDSCONNECTION__QUEUENAME=your-username.STDDS.your-uuid.OUT
